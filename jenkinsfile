pipeline {
    agent any
    
    stages {
        stage('Checkout') {
            steps {
                // Checkout the code from GitHub repository
                git branch: 'main', url: 'https://github.com/minhnhut-dev/hmus-mining.git'
            }
        }
        
        stage('Build') {
            steps {
                // Build your project (replace with your actual build command)
                sh 'docker build -t 22424025 .'
            }
        }
        
    }
}
